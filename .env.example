# Copy this file to `.env` (kept out of git) and adjust values for local development.
# These are safe defaults for running against a local echo/mock upstream.

# Upstream Umami target (host name only, no scheme)
UMAMI_HOST=localhost
UMAMI_PORT=1234
# Optional SNI if you point at TLS upstream (leave empty for plain HTTP)
UMAMI_SNI=

# Log level (tracing-subscriber reads RUST_LOG)
RUST_LOG=info

# Set to 1 to skip contacting a Kubernetes API (no k8s cluster locally)
DISABLE_K8S=1

# Listen ports for the services (override if ports are busy)
PROXY_LISTEN_PORT=6191
PROBE_LISTEN_PORT=6969
METRICS_LISTEN_PORT=9090
